{
  "name": "Socialize-starter",
  "version": "1.0.0",
  "description": "Meteor starter application utilizing Socialize packages.",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "lint": "eslint ./lib ./client ./server --ext .js",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha .scripts/mocha_boot.js client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9001"
  },
  "author": "Jan Dvorak",
  "dependencies": {
    "bcrypt": "^0.8.7",
    "mantra-core": "^1.2.0",
    "meteor-node-stubs": "^0.2.3",
    "moment": "^2.14.1",
    "react": "^15.3.0",
    "react-dom": "^15.0.0",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-simple-di": "^1.0.1",
    "redux": "^3.5.2",
    "sanitize-html": "^1.13.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.x.x",
    "enzyme": "^2.4.0",
    "jsdom": "^8.0.4",
    "eslint": "2.7.x",
    "eslint-plugin-react": "4.3.x",
    "eslint-plugin-meteor": "^3.6.x",
    "babel-core": "6.x.x",
    "babel-plugin-react-require": "2.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-meteor": "^6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "babel-root-slash-import": "1.x.x",
    "sinon": "1.17.x",
    "chai": "3.x.x",
    "mantra-lint": "^0.0.1",
    "mocha": "3.x.x",
    "react-addons-test-utils": "^15.0.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1"
  }
}
